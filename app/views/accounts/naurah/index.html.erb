<% content_for :head do %>
    <title>Paket Naurah | Lebih Dari Sekedar Nikmatnya Ibadah</title>
<% end %>

<% content_for :abutours do %>
    <section class="c-dashboard">
      <div class="container">
        <div class="row">
          <!-- --------------------------------------------------- -->
          <div class="col-3 pr-4 sticky">
            <div class="card c-dashboard-side pb-5">
              <div class="card-body c-dashboard-side__item p-0 theiaStickySidebar">
                <%= render template: 'accounts/partials/sidebar' %>
              </div>
            </div>
          </div>
          <!-- --------------------------------------------------- -->
          <div class="col-9 pl-0 pr-2 ml-15">
            <div class="_landing | w-breadcrumb">
              <ol class="breadcrumb border-bottom-0">
                <li class="breadcrumb-item"><a href="/">Halaman awal</a></li>
                <li class="breadcrumb-item active">Paket naurah</li>
              </ol>
            </div>
            <!-- --------------------------------------------------- -->
            <h3 class="mb-3">Semua Paket Naurah</h3>
            <div class="c-bg-white-accounts c-packetowner mt-0 mb-2">
              <div class="card c-dashboard-paket c-packetowner__item">
                <div class="card-body c-dashboard-paket__item c-packetowner__item--body p-0">
                  <table class="table table-paket m-0">
                    <tr>
                      <td class="c-paket__body">
                        <div class="media">
                          <%= image_tag 'http://office.abutours.com/Files/Paket/PRD220171114.png', :class => 'd-flex align-self-center mr-3', :alt => '' %>
                          <div class="media-body pt-2">
                            <p class="item-link item-line">
                              Paket umrah bersama oktober 2019 quad plus batulicin
                            </p>

                            <h4 class="mt-2 mb-0">Rp 30,000,000</h4>

                            <p class="card-text packet-city m-0">
                              <small class="text-muted">
                                Kota kalimantan - Zona E
                              </small>
                            </p>
                            <p class="card-text packet-city m-0">
                              <small class="text-muted">
                                Kantor Tanah bumbu
                              </small>
                            </p>
                          </div>
                        </div>
                      </td>

                      <td width="100" class="c-paket__footer">
                        <a href="" class="btn btn-block btn-danger">
                          Pesan Paket
                        </a>
                        <a href="/detail-naurah-customer" class="btn btn-block btn-outline-success">
                          Detail Paket
                        </a>
                      </td>
                    </tr>
                  </table>
                </div>
              </div>
            </div>

            <div class="c-bg-white-accounts c-packetowner mt-0 mb-2">
              <div class="card c-dashboard-paket c-packetowner__item">
                <div class="card-body c-dashboard-paket__item c-packetowner__item--body p-0">
                  <table class="table table-paket m-0">
                    <tr>
                      <td class="c-paket__body">
                        <div class="media">
                          <%= image_tag 'http://office.abutours.com/Files/Paket/PRD220171114.png', :class => 'd-flex align-self-center mr-3', :alt => '' %>
                          <div class="media-body pt-2">
                            <p class="item-link item-line">
                              Paket umrah bersama oktober 2019 quad plus batulicin
                            </p>

                            <h4 class="mt-2 mb-0">Rp 30,000,000</h4>

                            <p class="card-text packet-city m-0">
                              <small class="text-muted">
                                Kota kalimantan - Zona E
                              </small>
                            </p>
                            <p class="card-text packet-city m-0">
                              <small class="text-muted">
                                Kantor Tanah bumbu
                              </small>
                            </p>
                          </div>
                        </div>
                      </td>

                      <td width="100" class="c-paket__footer">
                        <a href="" class="btn btn-block btn-danger">
                          Pesan Paket
                        </a>
                        <a href="/detail-naurah-customer" class="btn btn-block btn-outline-success">
                          Detail Paket
                        </a>
                      </td>
                    </tr>
                  </table>
                </div>
              </div>
            </div>
          </div>
          <!-- --------------------------------------------------- -->
        </div>
      </div>
    </section>
<% end %>
<% content_for :foot do %>
    <script type="text/javascript">
        $(document).ready(function () {
            $(".js-example-placeholder-single").select2({});
        });
    </script>
<% end %>