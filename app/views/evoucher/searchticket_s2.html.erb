    <script>
        var options = [];
        var prices = [];
        var times = [];

        var options2 = [];
        var prices2 = [];
        var times2 = [];

        function initFilter() {
            options = [];
            prices = [<%= @harga_min ? @harga_min['price_value'] : 0 %>, <%= @harga_max ? @harga_max['price_value'] : 0 %>];
            times = ['00:00','24:00'];

            options2 = [];
            prices2 = [<%= @harga_min2 ? @harga_min2['price_value'] : 0 %>, <%= @harga_max2 ? @harga_max2['price_value'] : 0 %>];
            times2 = ['00:00','24:00'];
        }


        $(document).ready(function () {
            Number.prototype.formatMoney = function (c, d, t) {
                var n = this,
                    c = isNaN(c = Math.abs(c)) ? 2 : c,
                    d = d == undefined ? "." : d,
                    t = t == undefined ? "," : t,
                    s = n < 0 ? "-" : "",
                    i = String(parseInt(n = Math.abs(Number(n) || 0).toFixed(c))),
                    j = (j = i.length) > 3 ? j % 3 : 0;
                return s + (j ? i.substr(0, j) + t : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + t) + (c ? d + Math.abs(n - i).toFixed(c).slice(2) : "");
            };

            function pad(n, width, z) {
                z = z || '0';
                n = n + '';
                return n.length >= width ? n : new Array(width - n.length + 1).join(z) + n;
            }

            initFilter();

            $('a[id^=sort_]').each(function () {
                $(this).click(function () {
                    var klass = $(this).attr('class');
                    $('.' + klass).removeClass('active');
                    $(this>"div").removeClass('icon-updown').removeClass('icon-down').removeClass('icon-up').addClass('icon-updown');
                    $(this).addClass('active');
                    var sort_by = $(this).attr('id').substring(6);
                    var order = $(this).data('sort');
                    if (order === 'asc') {
                        $(this).data('sort', 'desc');
                        $(this).find("div").addClass('icon-up');
                    } else {
                        $(this).data('sort', 'asc');
                        $(this).find("div").addClass('icon-down');
                    }
                    var tabel = $(this).closest('.table-search').attr('id');
                    tinysort('#' + tabel + '>tbody>tr', {
                        data: sort_by,
                        order: (this.isAsc = !this.isAsc) ? 'asc' : 'desc'
                    });
                });
            });


            function doFilter(_table, _options, _times, _prices) {
                var $result = $('table#' + _table + '>tbody>tr');
                var len = _options.length;
                var $res1, $res2, $res3;
                $res1 = $result;
                $result.hide();
                if (len > 0) {
                    $res1 = $result.filter(function () {
                        return $.inArray($(this).data('airlinesshortname'), _options) > -1;
                    });
                }

                console.log($res1);
                $res2 = $res1.filter(function() {
                    return (
                        eval( $(this).data('price') ) >= _prices[0] &&
                        eval( $(this).data('price') ) <= _prices[1]
                    );
                });

                $res3 = $res2.filter(function() {
                    return (
                        $(this).data('depart') >= _times[0] &&
                        $(this).data('depart') <= _times[1]
                    );
                });

                $res3.show();
            }

            $('.dropdown-menu a.dropdown-item').on('click', function (event) {

                var $target = $(event.currentTarget),
                    val = $target.attr('data-value'),
                    btn = $target.attr('data-button'),
                    rst = $target.attr('data-reset'),
                    $inp = $target.find('input'),
                    tbl = $target.attr('data-table'),
                    idx;

                if (tbl === 'search_res') {
                    if (( idx = options.indexOf(val) ) > -1) {
                        options.splice(idx, 1);
                        setTimeout(function () {
                            $inp.prop('checked', false)
                        }, 0);
                    } else {
                        options.push(val);
                        setTimeout(function () {
                            $inp.prop('checked', true)
                        }, 0);
                    }

                    if (options.length <= 0) {
                        $('#' + btn).removeClass('active');
                    } else {
                        $('#' + btn).addClass('active');
                    }
                } else {
                    if (( idx = options2.indexOf(val) ) > -1) {
                        options2.splice(idx, 1);
                        setTimeout(function () {
                            $inp.prop('checked', false)
                        }, 0);
                    } else {
                        options2.push(val);
                        setTimeout(function () {
                            $inp.prop('checked', true)
                        }, 0);
                    }

                    if (options2.length <= 0) {
                        $('#' + btn).removeClass('active');
                    } else {
                        $('#' + btn).addClass('active');
                    }
                }
                $(event.target).blur();

                console.log(tbl === 'search_res');
                console.log(tbl === 'search_res2');

                if (tbl === 'search_res') {
                    doFilter(tbl, options, times, prices);
                } else {
                    doFilter(tbl, options2, times2, prices2);
                }
                $('#' + rst).show();
                return false;
            });

            $("#filterPrice1").bootstrapSlider();
            $("#filterTime1").bootstrapSlider();
            $("#filterPrice2").bootstrapSlider();
            $("#filterTime2").bootstrapSlider();

            $("#filterPrice1").on("change", function (event) {
                var a = event.value.newValue;
                var b = event.value.oldValue;

                var changed = !($.inArray(a[0], b) !== -1 &&
                    $.inArray(a[1], b) !== -1 &&
                    $.inArray(b[0], a) !== -1 &&
                    $.inArray(b[1], a) !== -1 &&
                    a.length === b.length);

                if( changed ) {
                    prices[0] = event.value.newValue[0];
                    prices[1] = event.value.newValue[1];
                    $("#priceLow").text('IDR ' + prices[0].formatMoney(0, ',', '.'));
                    $("#priceHigh").text('IDR ' + prices[1].formatMoney(0, ',', '.'));
                    $('#ddHarga').addClass('active');
                    doFilter('search_res', options, times, prices);
                    $('#resetFilter').show();
                }
            });

            $("#filterTime1").on("change", function (event) {
                var a = event.value.newValue;
                var b = event.value.oldValue;

                var changed = !($.inArray(a[0], b) !== -1 &&
                    $.inArray(a[1], b) !== -1 &&
                    $.inArray(b[0], a) !== -1 &&
                    $.inArray(b[1], a) !== -1 &&
                    a.length === b.length);

                if( changed ) {
                    jam1 = pad(event.value.newValue[0], 2) + ':00';
                    jam2 = pad(event.value.newValue[1], 2) + ':00';
                    times[0] = jam1;
                    times[1] = jam2;
                    $("#timeLow").text(jam1);
                    $("#timeHigh").text(jam2);
                    $('#ddWaktu').addClass('active');
                    doFilter('search_res', options, times, prices);
                    $('#resetFilter').show();
                }
            });

            $("#filterPrice2").on("change", function (event) {
                var a = event.value.newValue;
                var b = event.value.oldValue;

                var changed = !($.inArray(a[0], b) !== -1 &&
                    $.inArray(a[1], b) !== -1 &&
                    $.inArray(b[0], a) !== -1 &&
                    $.inArray(b[1], a) !== -1 &&
                    a.length === b.length);

                if( changed ) {
                    prices2[0] = event.value.newValue[0];
                    prices2[1] = event.value.newValue[1];
                    $("#priceLow2").text('IDR ' + prices2[0].formatMoney(0, ',', '.'));
                    $("#priceHigh2").text('IDR ' + prices2[1].formatMoney(0, ',', '.'));
                    $('#ddHarga2').addClass('active');
                    doFilter('search_res2', options2, times2, prices2);
                    $('#resetFilter2').show();
                }
            });

            $("#filterTime2").on("change", function (event) {
                var a = event.value.newValue;
                var b = event.value.oldValue;

                var changed = !($.inArray(a[0], b) !== -1 &&
                    $.inArray(a[1], b) !== -1 &&
                    $.inArray(b[0], a) !== -1 &&
                    $.inArray(b[1], a) !== -1 &&
                    a.length === b.length);

                if( changed ) {
                    jam1 = pad(event.value.newValue[0], 2) + ':00';
                    jam2 = pad(event.value.newValue[1], 2) + ':00';
                    times2[0] = jam1;
                    times2[1] = jam2;
                    $("#timeLow2").text(jam1);
                    $("#timeHigh2").text(jam2);
                    $('#ddWaktu2').addClass('active');
                    doFilter('search_res2', options2, times2, prices2);
                    $('#resetFilter2').show();
                }
            });


            $('#resetFilter').click(function () {
                initFilter();
                $('table#search_res>tbody>tr').show();

                $("input:checkbox").attr( "checked" , false );
                $("#filterPrice1").bootstrapSlider('refresh');
                $("#filterTime1").bootstrapSlider('refresh');

                $('#ddMaskapai').removeClass('active');
                $('#ddWaktu').removeClass('active');
                $('#ddHarga').removeClass('active');

                $(this).hide();
            });

            $('#resetFilter2').click(function () {
                initFilter();
                $('table#search_res2>tbody>tr').show();

                $("input:checkbox").attr( "checked" , false );
                $("#filterPrice2").bootstrapSlider('refresh');
                $("#filterTime2").bootstrapSlider('refresh');

                $('#ddMaskapai2').removeClass('active');
                $('#ddWaktu2').removeClass('active');
                $('#ddHarga2').removeClass('active');

                $(this).hide();
            })

        });

    </script>

<% end %>